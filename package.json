{
  "name": "my-vite-board-clean-code",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "====================": "개발실행명령어",
    "dev": "vite",
    "dev:mock": "vite --mode mock",
    "=======================": "mock 서버 실행",
    "mock": "yarn nodemon --config nodemon.json",
    "==========================": "concurrently \"yarn dev-vite\" \"yarn nodemon\"",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "ts-check": "tsc --project tsconfig.app.json",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@lukemorales/query-key-factory": "^1.3.4",
    "@sentry/react": "^8.34.0",
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/query-core": "^5.51.15",
    "@tanstack/react-query": "^5.51.11",
    "@tanstack/react-query-devtools": "^5.51.11",
    "axios": "^1.7.7",
    "immer": "^10.1.1",
    "kim-gui": "2.0.5",
    "msw": "^2.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-router-dom": "^6.25.1",
    "recoil": "^0.7.7",
    "styled-components": "^6.1.12",
    "swiper": "^11.2.6",
    "tailwindcss": "^4.1.7",
    "vite-plugin-circular-dependency": "^0.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.1.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vitest": "^2.0.5",
    "wiremock": "^3.9.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}